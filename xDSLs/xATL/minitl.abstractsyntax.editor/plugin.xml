<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated minitl -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="minitl.presentation.minitlModelWizardID"
            name="%_UI_minitlModelWizard_label"
            class="minitl.presentation.minitlModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/minitlModelFile.gif">
         <description>%_UI_minitlModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated minitl -->
      <editor
            id="minitl.presentation.minitlEditorID"
            name="%_UI_minitlEditor_label"
            icon="icons/full/obj16/minitlModelFile.gif"
            extensions="minitl"
            class="minitl.presentation.minitlEditor"
            contributorClass="minitl.presentation.minitlActionBarContributor">
      </editor>
   </extension>

</plugin>
