ruleset ArduinoBranchCoverage{
	
	import metamodel arduino
	import ruleset ArduinoModelElementCoverage

	context If{
		when "self.elseBlock <> null"{
			branch "self.block",
			branch "self.elseBlock"
		}, 
		when "self.elseBlock = null"{
			branch "self.block",
			branch else
		}
	},
	context Repeat{
		{
			branch "self.block",
			branch else
		}
	},
	context While{
		{
			branch "self.block",
			branch else
		}
	}
}