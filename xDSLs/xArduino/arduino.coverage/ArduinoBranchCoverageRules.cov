ruleset ArduinoBranchCoverage {
	import metamodel arduino
	context If {
		when "self.elseBlock <> null" {
			branch "self.block",
			branch "self.elseBlock"
			
		}, 
		when "self.elseBlock = null"{
			branch "self.block",//Comment this Error: one explicit branch
			branch else
			//,branch  else//Comment this Error: one implicit branch
		} 
	},
	context While {
		{
			branch "self.block",
			branch else 
		}
	}
}