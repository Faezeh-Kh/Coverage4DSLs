<?xml version="1.0" encoding="ASCII"?>
<DSLSpecificCoverage:DomainSpecificCoverage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:DSLSpecificCoverage="https://coverage/DSLSpecificCoverage" name="MiniJavaCoverage">
  <contexts>
    <metaclass href="http://minijava.xminijava/xminiJava/#//Program"/>
    <rules xsi:type="DSLSpecificCoverage:Ignore"/>
  </contexts>
  <contexts>
    <metaclass href="http://minijava.xminijava/xminiJava/#//Method"/>
    <rules xsi:type="DSLSpecificCoverage:Ignore"/>
  </contexts>
  <contexts>
    <metaclass href="http://minijava.xminijava/xminiJava/#//MethodCall"/>
    <rules xsi:type="DSLSpecificCoverage:IgnoreIfContained" condition="EXCLUSION">
      <containerType href="http://minijava.xminijava/xminiJava/#//Block"/>
    </rules>
  </contexts>
  <contexts>
    <metaclass href="http://minijava.xminijava/xminiJava/#//Class"/>
    <rules xsi:type="DSLSpecificCoverage:CoverageByContainment" multiplicity="ONE">
      <containmentReference href="http://minijava.xminijava/xminiJava/#//TypeDeclaration/members"/>
    </rules>
  </contexts>
  <metamodel href="http://minijava.xminijava/xminiJava/#/"/>
</DSLSpecificCoverage:DomainSpecificCoverage>
