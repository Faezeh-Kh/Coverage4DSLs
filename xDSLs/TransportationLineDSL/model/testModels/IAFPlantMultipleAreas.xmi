<?xml version="1.0" encoding="UTF-8"?>
<tll:System
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:tll="http://www.example.org/tll"
    xsi:schemaLocation="http://www.example.org/tll ../metamodel/tll.ecore"
    id="IAFPlant">
  <area id="Area1"
      store="//@area.0/@component.9"
      buffer="//@area.0/@component.8">
    <component
        xsi:type="tll:Turntable"
        id="a1_t1"
        processingTime="500"
        uncertainty="1"
        neighbour="//@area.0/@component.1"/>
    <component
        xsi:type="tll:Conveyor"
        id="a1_c1"
        processingTime="200"
        uncertainty="4"
        neighbour="//@area.0/@component.2 //@area.0/@component.0"/>
    <component
        xsi:type="tll:Turntable"
        id="a1_t2"
        processingTime="500"
        uncertainty="10"
        neighbour="//@area.0/@component.3 //@area.0/@component.1"/>
    <component
        xsi:type="tll:Machine"
        id="a1_m1"
        processingTime="1000"
        uncertainty="15"
        neighbour="//@area.0/@component.4 //@area.0/@component.2"/>
    <component
        xsi:type="tll:Turntable"
        id="a1_t3"
        processingTime="500"
        uncertainty="10"
        neighbour="//@area.0/@component.5 //@area.1/@component.2 //@area.0/@component.3"/>
    <component
        xsi:type="tll:Conveyor"
        id="a1_c2"
        processingTime="200"
        uncertainty="4"
        neighbour="//@area.0/@component.6 //@area.0/@component.4"/>
    <component
        xsi:type="tll:Turntable"
        id="a1_t4"
        processingTime="500"
        uncertainty="10"
        neighbour="//@area.0/@component.7 //@area.1/@component.0 //@area.0/@component.5"/>
    <component
        xsi:type="tll:Conveyor"
        id="a1_c3"
        processingTime="200"
        uncertainty="4"
        neighbour="//@area.0/@component.0 //@area.0/@component.6"/>
    <component
        xsi:type="tll:WaitingQueue"
        id="a1_buffer"
        processingTime="2"
        uncertainty="2"
        entry="//@area.0/@component.0"/>
    <component
        xsi:type="tll:StorageQueue"
        id="a1_store"
        processingTime="2"
        uncertainty="2"
        exit="//@area.0/@component.6"/>
    <itemGen
        frequency="30"/>
  </area>
  <area id="Area2"
      store="//@area.1/@component.9"
      buffer="//@area.1/@component.8">
    <component
        xsi:type="tll:Turntable"
        id="a2_t1"
        processingTime="500"
        uncertainty="30"
        neighbour="//@area.1/@component.1 //@area.0/@component.6"/>
    <component
        xsi:type="tll:Conveyor"
        id="a2_c1"
        processingTime="2000"
        uncertainty="40"
        neighbour="//@area.1/@component.2 //@area.1/@component.0"/>
    <component
        xsi:type="tll:Turntable"
        id="a2_t2"
        processingTime="500"
        uncertainty="30"
        neighbour="//@area.1/@component.3 //@area.0/@component.4 //@area.1/@component.1"/>
    <component
        xsi:type="tll:Machine"
        id="a2_m1"
        processingTime="10000"
        uncertainty="150"
        neighbour="//@area.1/@component.4 //@area.1/@component.2"/>
    <component
        xsi:type="tll:Turntable"
        id="a2_t3"
        processingTime="500"
        uncertainty="30"
        neighbour="//@area.1/@component.5 //@area.2/@component.2 //@area.1/@component.3"/>
    <component
        xsi:type="tll:Conveyor"
        id="a2_c2"
        processingTime="2000"
        uncertainty="40"
        neighbour="//@area.1/@component.6 //@area.1/@component.4"/>
    <component
        xsi:type="tll:Turntable"
        id="a2_t4"
        processingTime="500"
        uncertainty="30"
        neighbour="//@area.1/@component.7 //@area.2/@component.0 //@area.1/@component.5"/>
    <component
        xsi:type="tll:Conveyor"
        id="a2_c3"
        processingTime="2000"
        uncertainty="40"
        neighbour="//@area.1/@component.0 //@area.1/@component.6"/>
    <component
        xsi:type="tll:WaitingQueue"
        id="a2_buffer"
        processingTime="20"
        uncertainty="5"
        entry="//@area.1/@component.0"/>
    <component
        xsi:type="tll:StorageQueue"
        id="a2_store"
        processingTime="20"
        uncertainty="5"
        exit="//@area.1/@component.6"/>
    <itemGen
        frequency="25"/>
  </area>
  <area id="Area3"
      store="//@area.2/@component.9"
      buffer="//@area.2/@component.8">
    <component
        xsi:type="tll:Turntable"
        id="a3_t1"
        processingTime="500"
        uncertainty="1"
        neighbour="//@area.2/@component.1 //@area.1/@component.6"/>
    <component
        xsi:type="tll:Conveyor"
        id="a3_c1"
        processingTime="300"
        uncertainty="4"
        neighbour="//@area.2/@component.2 //@area.2/@component.0"/>
    <component
        xsi:type="tll:Turntable"
        id="a3_t2"
        processingTime="50"
        uncertainty="10"
        neighbour="//@area.2/@component.3 //@area.1/@component.4 //@area.2/@component.1"/>
    <component
        xsi:type="tll:Machine"
        id="a3_m1"
        processingTime="1000"
        uncertainty="150"
        neighbour="//@area.2/@component.4 //@area.2/@component.2"/>
    <component
        xsi:type="tll:Turntable"
        id="a3_t3"
        processingTime="50"
        uncertainty="10"
        neighbour="//@area.2/@component.5 //@area.2/@component.3"/>
    <component
        xsi:type="tll:Conveyor"
        id="a3_c2"
        processingTime="200"
        uncertainty="4"
        neighbour="//@area.2/@component.6 //@area.2/@component.4"/>
    <component
        xsi:type="tll:Turntable"
        id="a3_t4"
        processingTime="500"
        uncertainty="10"
        neighbour="//@area.2/@component.7 //@area.2/@component.5"/>
    <component
        xsi:type="tll:Conveyor"
        id="a3_c3"
        processingTime="200"
        uncertainty="4"
        neighbour="//@area.2/@component.0 //@area.2/@component.6"/>
    <component
        xsi:type="tll:WaitingQueue"
        id="a3_buffer"
        processingTime="2"
        uncertainty="2"
        entry="//@area.2/@component.0"/>
    <component
        xsi:type="tll:StorageQueue"
        id="a3_store"
        processingTime="2"
        uncertainty="2"
        exit="//@area.2/@component.6"/>
    <itemGen
        frequency="3"/>
  </area>
  <simconfig
      timeout="85000"/>
</tll:System>
